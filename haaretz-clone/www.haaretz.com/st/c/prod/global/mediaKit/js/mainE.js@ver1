//--------------------------------------------------------------//
//       Show And Replace Content Details
//--------------------------------------------------------------//

	$(document).click(".text-option", function (e) {
		if(isPlay){
			inter_players["tVTj5YBzW5Q"].pauseVideo();
		}
		e.stopPropagation();
		var opt_index = $(e.target).attr('data-options');



		$("#presentation").fadeOut(500);


		// if (opt_index == 0 && tabKey == "pro8" || opt_index == 0 && tabKey == "pro9")
		// {
		// 	$(".text-option").removeClass('intro-eng');
		// 	$(e.target).addClass('intro-np');
		// }
		// else {
		//
		// 	$(".text-option").removeClass('intro-eng');
		// 	$(e.target).addClass('intro-eng');
		// }




		if (!contentStatus) {
			$("#media-cont #content").fadeIn(500).css("display", "block");
			contentStatus = true;
		}

		$(".prod-title-eng").html($DataHeb[tabKey][opt_index].title);
		$(".prod-details .details").html($DataHeb[tabKey][opt_index].details);


		if ($DataHeb[tabKey][opt_index].size == null) {
			$(".divSize").html("");
		        }

		else
		{
			$(".divSize").html("<span>size: </span><span class='size'></span>");
			$(".prod-details .size").html($DataHeb[tabKey][opt_index].size);
		}


		if ($DataHeb[tabKey][opt_index].url == null) {
			$(".close").html("");
		}

		else
		{
			$(".close").html("<a id='closeUrl' href='#'>הוראות סגירה</a>");
			$("a#closeUrl").attr("href", "//www.themarker.com/st/c/prod/global/mediaKit/close.html#" + $DataHeb[tabKey][opt_index].url);
		}



		if ($DataHeb[tabKey][opt_index].img == null) {
			$(".img-content").css("display","none");
			$("#media-cont #content .text-content").css("width", "100%");
			$("#imgDetails").empty();
		}
		else {
			$(".img-content").css("display","inline-block");
			$("#media-cont #content .text-content").css("width", "30%");
			var newImg = "//www.haaretz.co.il/st/c/static/global/media/haaretzCom/" + $DataHeb[tabKey][opt_index].img;
			$(".img-content img").attr("src", newImg);
		}




		if ( $(".text-content").height() > 391 && $DataHeb[tabKey][opt_index].img != null)
		{
			$(".img-content").css("display","none");
			$("#media-cont #content .text-content").css("width", "100%");
			$("#media-cont #content .text-content").css("float", "left");
			var strimg = "<img src='"+ newImg +"'>";
			$( strimg ).appendTo( ".prod-details #imgDetails" );

		}
		else if ( $(".text-content").height() < 391 && $DataHeb[tabKey][opt_index].img != null)
		{
			$(".img-content").css("display","inline-block");
			$("#media-cont #content .text-content").css("width", "30%");
			$("#media-cont #content .text-content").css("float", "none");
			$("#imgDetails").empty();
		}
	});


$(document).bind("keypress", ".text-option", function (e) {
	if(event.keyCode == 13){
		$(e.target).click();
	}
});


	$(document).on("click", ".product-box", function (e) {
		e.stopPropagation();
	});



