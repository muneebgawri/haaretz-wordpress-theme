// $(document).on("mouseover", ".product-box", function (e) {
// 	$(".product-box").removeClass('boxHover');
// 	$(this).addClass('boxHover');
// });


	$(document).on("click", ".product-box", function (e) {

	   $("#media-cont #content").css("display", "none");
	   contentStatus = false;
	   $(".product-box").removeClass('boxHover');
	   $(this).addClass('boxHover');
	   var box_index = $(this).index();
	   tabKey = "pro" + (box_index);

	   if (box_index == 0) {
		   if(isPlay){
			   inter_players["tVTj5YBzW5Q"].playVideo();
		   }
		   $(".option-box").css("visibility", "hidden");
		   return;
	   }
	   else {

		   //$("#presentation").fadeOut(500);
		   $(".option-box").css("visibility", "visible");
	   }

	   for (var i = 0; i < $DataHeb[tabKey].length; i++) {

		   // if (i == 0) {
			//    if (tabKey == "pro8" || tabKey == "pro9")
			//    {
			// 	   strOption += "<div aria-label='" + $DataHeb[tabKey][i].name + "' data-tab='" + tabKey + "' data-options='" + i + "' class='text-option intro-np'>" + $DataHeb[tabKey][i].name + "</div>";
			// 	   i++;
			//    }
			//    else
			//    {
			// 	   strOption += "<div aria-label='" + $DataHeb[tabKey][i].name + "' data-tab='" + tabKey + "' data-options='" + i + "' class='text-option'>" + $DataHeb[tabKey][i].name + "</div>";
			// 	   i++;
			//    }
		   // }


		   strOption += "<div aria-label='" + $DataHeb[tabKey][i].name + "' data-tab='" + tabKey + "' data-options='" + i + "' class='text-option eng'>" + $DataHeb[tabKey][i].name + "</div>";
	   }
	   $(".option-box").html(strOption);
	   //$("#presentation").fadeOut(500);

	    $(".option-box").css("visibility", "visible");
	      strOption = "";

		var n =1;

		$(".text-option").each(function (n) { $(this).attr('tabindex', n + 1); });

	});


//---------------------  EVENTS  -------------------------------//
//       Play the main youtube of every site
//--------------------------------------------------------------//

	$(document).on("click", ".product-box:first-child", function (event) {

		event.stopPropagation();
		$(".option-box").css("visibility", "hidden");
		$("#media-cont #content").css("display", "none");
		$("#presentation").fadeIn(550).css("display", "block");
		contentStatus = false;
		$(".option-box .text-option").removeClass('intro');
	});


