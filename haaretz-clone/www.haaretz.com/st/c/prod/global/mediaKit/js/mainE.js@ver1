//--------------------------------------------------------------//
//       Form Validation
//--------------------------------------------------------------//

function FormValidation() {
	//Validate Field 1
	if (firstName.value == "") {
		alert("נא למלא שם      ");
		firstName.focus();
		return false;
	}
	if (lastName.value == "") {
		alert("נא למלא שם משפחה");
		lastName.focus();
		return false;
	}

	if (phone2.value.length < 7) {
		alert("נא למלא 7 ספרות")
		phone2.focus();
		return false;
	}

	var firstmonber = selectNo.options[selectNo.selectedIndex].value;
	if (firstmonber == "בחר"){
		alert("נא לבחור קידומת");
		selectNo.focus();
		return false;
	}


	var emailRegEx = /^[A-Z0-9._%+-]+@[A-Z0-9.-]+\.[A-Z]{2,4}$/i;
	if (!(email.value.search(emailRegEx) != -1) && (email.value.length < 64)) {
		alert("נא למלא כתובת מייל תקינה");
		email.focus();
		return false;
	}
	var str1 = firstName.value + " ";
	var str2 = lastName.value;
	var res = str1.concat(str2);
	fullName.value = res;


	var fullPhone = (firstmonber + phone2.value);
	phone.value = fullPhone;





}



//--------------------------------------------------------------//
//       Form Validation Eng
//--------------------------------------------------------------//

function FormValidationEng() {

	if (firstNameEng.value == "") {
		alert("Enter your first name");
		firstNameEng.focus();
		return false;
	}
	if (lastNameEng.value == "") {
		alert("Enter your Last name");
		lastNameEng.focus();
		return false;
	}

	var str1 = firstNameEng.value + " ";
	var str2 = lastNameEng.value;
	var res = str1.concat(str2);
	fullName.value = res;


	if (phone.value.length < 7) {
		alert("Enter 7 numbers");
		phone.focus();
		return false;
	}


	var emailRegEx = /^[A-Z0-9._%+-]+@[A-Z0-9.-]+\.[A-Z]{2,4}$/i;
	if (!(emailEng.value.search(emailRegEx) != -1) && (email.value.length < 64)) {
		alert("Incorrect email");
		emailEng.focus();
		return false;
	}

}


function backToMainEng() {
	window.location.assign("//www.haaretz.com/st/c/prod/global/mediaKit/haaretzEng.html");
}



$(document).on("click", "#contactH", function (event) {
	if(isPlay){inter_players["tVTj5YBzW5Q"].pauseVideo();}
	event.stopPropagation();
	$(".site-box-button").removeClass('active');
	$(this).addClass('active');


	$("#products-group").css("display", "none");
	$("#products").css("display", "none");

	$("#contactUs").fadeIn(550).css("display", "block");
	$("#media-cont").fadeIn(550).css("display", "none");
	$("#contactH").css("color", "black");

	$("#contactH").css("padding", "0 15px");


});

$(document).on("click", "#contactE", function (event) {
	if(isPlay){inter_players["tVTj5YBzW5Q"].pauseVideo();}
	event.stopPropagation();
	$(".site-box-button").removeClass('active');
	$(this).addClass('active');


	$("#products-group").css("display", "none");
	$("#products").css("display", "none");

	$("#contactUsEng").fadeIn(550).css("display", "block");
	$("#media-cont").fadeIn(550).css("display", "none");
	$("#contactE").css("color", "black");

	$("#contactE").css("padding", "0 15px");


});



// 2. This code loads the IFrame Player API code asynchronously.
var tag = document.createElement('script');
tag.src = "//www.youtube.com/player_api";
var firstScriptTag = document.getElementsByTagName('script')[0];
firstScriptTag.parentNode.insertBefore(tag, firstScriptTag);

