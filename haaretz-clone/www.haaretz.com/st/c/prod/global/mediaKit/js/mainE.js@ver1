function videoReady() {

}

$(document).ready(function(){
	var currentID = $('.inter-video-mask').attr('id');

	$('#'+currentID).parent().addClass('fakeyoutubebg').css("background-image","url('//www.haaretz.co.il/st/c/static/global/media/img/hEngN.jpg')")


	var youtubelogo = document.createElement("img");
	youtubelogo.src = '//www.haaretz.co.il/st/c/static/resources/img/items/logo-youtube.png';
	youtubelogo.setAttribute('class','youtubeLogo');
	$('.inter-video-mask').append(youtubelogo);

	var tempButton = document.createElement("img");
	tempButton.src = '//www.haaretz.co.il/st/c/static/resources/img/items/youtubeplay.png';
	tempButton.setAttribute('class','youtubeplayButton');
	tempButton.id = 'butt' + currentID;
	$('.inter-video-mask').append(tempButton);

	var $youtubeLink = "https://www.youtube.com/embed/" + currentID;
	var $mobileYoutubeLink = "//m.youtube.com/watch?v=" + currentID;
//var tempEmbedStr = '<iframe src="https://www.youtube.com/embed/' + currentID +'?playlist=' + currentID +'&controls=0&autoplay=0&wmode=transparent&loop=0&modestbranding=1&rel=0&showinfo=0" frameborder="0" allowfullscreen></iframe>'
	var tempEmbedStr = '<iframe src="' + $youtubeLink + '" frameborder="0"></iframe>'

	$('#'+tempButton.id).on('click',function(){
		isPlay = true;
		$('#' + currentID).html(tempEmbedStr);
		inter_players[currentID] = new YT.Player(currentID, {
			videoId: currentID,
			events: {
				'onReady': onPlayerReady
			}
		});
	})


	function onPlayerReady(e){
		console.log(e);
		e.target.playVideo();

	}
});



helloObj.func.projReady = function() {

	$('.inter-mc').show();
	$DataHeb = helloObj.data.eng;
	$( "#menu" ).slideDown(600);

	createMainMenuObj();
	//initGroupMenu();
	initMenu();


}
// -------------------------------------------------------------
//     Init the main Obj menu
// -------------------------------------------------------------

	function createMainMenuObj() {
		objMedia.mainProduct = [];
		for (i = 0; i < $DataHeb["product"].length; i++) {
			objMedia.mainProduct[i] = $DataHeb['product'][i].menu
		}

	}

// -------------------------------------------------------------
//     Init the content Obj
// -------------------------------------------------------------
	function CreateContentObj() {
		var key = "pro3";
		for (i = 0; i < $DataHeb[key].length; i++) {
			objSubMenu.push = {name: $DataHeb[key][i].name};
		}
	}

// -------------------------------------------------------------
//     Init group menu
// -------------------------------------------------------------

function initGroupMenu() {

	strGroup ="<div class='product-box-group' data-menugroup='group'><div class='text-prod-group active-main-menu'>הקדמה</div></div>" +
	          "<div class='product-box-group' data-menugroup='heb'><div class='text-prod-group'>הארץ</div></div>" +
			  "<div class='product-box-group' data-menugroup='mar'><div class='text-prod-group'>TheMarher</div></div>" +
			  "<div class='product-box-group' data-menugroup='eng'><div class='text-prod-group'>Haaretz.Com</div></div>" +
			  "<div class='product-box-group' data-menugroup='fin'><div class='text-prod-group'>finance</div></div>";


	$("#products-group").html(strGroup);
}

// -------------------------------------------------------------
//     Init the main menu
// -------------------------------------------------------------

	function initMenu() {

		for (i = 0; i < $DataHeb["product"].length; i++) {
			if (i == 0) {
				strMainMenu += "<div class='product-box' data-menu=" + i + ">" +
						"<div class='text-prod active-main-menu' aria-label='" + $DataHeb['product'][i].menu + "'>" + $DataHeb['product'][i].menu + "</div></div>";
				//createMainMenuObj($DataHeb['product'][i].menu, i);
			}
			else {
				strMainMenu += "<div class='product-box' data-menu=" + i + ">" +
						"<div class='text-prod' aria-label='" + $DataHeb['product'][i].menu + "'>" + $DataHeb['product'][i].menu + "</div>" +
						"<div class='icon-prod'><button class='eng'>}</button></div></div>";
				//createMainMenuObj($DataHeb['product'][i].menu, i);
				//objMedia.mainProduct[i] = $mediaData['product'][i].menu;
			}
		}
		$("#products").html(strMainMenu);
		strMainMenu = "";


		$menu = $("#products");

		$menu.menuAim({
			activate: activateSubmenu,
			deactivate: deactivateSubmenu,
			submenuDirection: "left"
		});

		function activateSubmenu(row) {
			var $row = $(row);
			var $submenu = $row.find('.option-box');
			console.log("$row" , $row);
			$submenu.css({
				display: "block"
			});

		}

		function deactivateSubmenu(row) {
			var $row = $(row),
					$submenu = $row.find('.option-box');

			// Hide the submenu and remove the row's highlighted look
			$submenu.css({
				display: "none"
			});

			//$row.removeClass("maintainHover");
		}

		$("#products .product-box").click(function(e) {
			e.stopPropagation();
		});





		/*for (key in $DataHeb) {
		 if (key != "product") {
		 for (var i = 0; i < $DataHeb[key].length; i++) {
		 objSubMenu.name = $DataHeb[key][i].name;
		 objSubMenu.title = $DataHeb[key][i].title;
		 objSubMenu.details = $DataHeb[key][i].details;
		 objSubMenu.size = $DataHeb[key][i].size;
		 objSubMenu.img = $DataHeb[key][i].img;
		 }
		 }
		 }*/
	}


