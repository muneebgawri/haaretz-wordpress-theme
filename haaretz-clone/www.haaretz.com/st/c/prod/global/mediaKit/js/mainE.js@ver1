var defaults = {
    loc: "media",
			bundleUrl: "//" + document.domain + "/st/c/work/bundle/4/3/",
			staticUrl: "//" + document.domain + "/st/c/static/eng/2015/maps/settlements/"

    },

settings = {
    dataFiles: [

	  'eng'

    ],
